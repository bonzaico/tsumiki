[access]
	inheritFrom = All-Projects
[submit]
	action = fast forward only
[project]
	description = Design system for Bonzai applications
[access "refs/*"]
	owner = group bonzai-engineering-owners
	owner = group user/Bonzai (bonzaidev)
[access "refs/for/refs/heads/*"]
	push = group bonzai-engineering
	pushMerge = group bonzai-engineering
[access "refs/heads/*"]
	addPatchSet = group bonzai-engineering
	forgeCommitter = group bonzai-engineering
	label-Code-Review = -2..+2 group bonzai-engineering
	label-Verified = -1..+1 group bonzai-engineering
	read = group bonzai-engineering
	submit = group bonzai-engineering
[access "refs/tags/*"]
	read = group bonzai-engineering
	createTag = group bonzai-engineering-owners
	createTag = group user/sayali-15
	forgeCommitter = group bonzai-engineering-owners
	forgeCommitter = group user/sayali-15
	push = group bonzai-engineering-owners
	push = group user/sayali-15
	createSignedTag = group bonzai-engineering-owners
	createSignedTag = group user/sayali-15
